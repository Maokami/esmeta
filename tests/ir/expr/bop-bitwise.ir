let x = 1.2
let y = 2.0
let ix = 3i
let iy = 5i
let bx = 1n
let by = 2n

assert (= (& ix iy) 1i)
assert (= (| ix iy) 7i)
assert (= (^ ix iy) 6i)
assert (= (<< ix iy) 96i)
assert (= (>> 16i 2i) 4i)
assert (= (>>> -1i 60i) 15i)

assert (= (& bx by) 0n)
assert (= (| bx by) 3n)
assert (= (^ bx by) 3n)
assert (= (<< bx by) 4n)
assert (= (>> 16n 2n) 4n)
